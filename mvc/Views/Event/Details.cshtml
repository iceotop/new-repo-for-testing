@model mvc.ViewModels.Event.EventDetailsViewModel

<section>
    <div>
        <span class="back-icon">
            <a asp-action="index"><span><i class="fa-regular fa-arrow-left-long"></i> Tillbaka</span></a>
        </span>
        <h2>@Model.Title</h2>
        <div>
            <span><strong>Title</strong></span>
            <span>@Model.Title</span>
            <span><strong>Book</strong></span>
            <span>@Model.Book</span>
            <span><strong>StartDate</strong></span>
            <span>@Model.StartDate</span>
            <span><strong>EndDate</strong></span>
            <span>@Model.EndDate</span>
        </div>
        <h4>Description</h4>
        <div>@Model.Description</div>
    </div>
</section>
<input type="text" id="txtUser" placeholder="Användarnamn" />
<input type="text" id="txtMessage" placeholder="Skriv ditt meddelande här" />
<button id="btnSend">Skicka</button>

<ul id="messages"></ul>

<script src="\js\signalr\dist\browser\signalr.js"></script>
<script src="\js\site.js"></script>